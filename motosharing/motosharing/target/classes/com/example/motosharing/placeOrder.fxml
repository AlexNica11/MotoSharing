      <Label layoutX="41.0" layoutY="340.0" text="Time">
         <font>
            <Font name="AppleMyungjo Regular" size="24.0" />
         </font></Label>
      <ComboBox fx:id="type" layoutX="109.0" layoutY="146.0" prefHeight="25.0" prefWidth="79.0" />
