<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="530.0" prefWidth="510.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.motosharing.controllers.OrderBikesController">
   <children>
      <Label layoutX="165.0" layoutY="39.0" text="Place Order">
         <font>
            <Font name="AppleMyungjo Regular" size="34.0" />
         </font>
      </Label>
      <Label layoutX="42.0" layoutY="142.0" text="Type">
         <font>
            <Font name="AppleMyungjo Regular" size="24.0" />
         </font></Label>
      <Label layoutX="43.0" layoutY="242.0" text="Quantity ">
         <font>
            <Font name="AppleMyungjo Regular" size="24.0" />
