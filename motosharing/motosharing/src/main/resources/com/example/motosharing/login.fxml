<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="530.0" prefWidth="510.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.motosharing.controllers.LoginController">
   <children>
      <Label layoutX="177.0" layoutY="39.0" text="LOG IN" underline="true">
         <font>
            <Font name="AppleMyungjo Regular" size="53.0" />
         </font>
      </Label>
      <Label layoutX="46.0" layoutY="157.0" text="Username/E-mail">
         <font>
            <Font name="AppleMyungjo Regular" size="18.0" />
         </font>
      </Label>
      <Label layoutX="46.0" layoutY="229.0" text="Password">
         <font>
            <Font name="AppleMyungjo Regular" size="18.0" />
         </font>
      </Label>
      <Button layoutX="351.0" layoutY="415.0" mnemonicParsing="false" onAction="#handleVerify" prefHeight="46.0" prefWidth="98.0" text="Verify">
         <font>
            <Font name="AppleMyungjo Regular" size="18.0" />
         </font>
      </Button>
      <TextField fx:id="usernameField" layoutX="212.0" layoutY="155.0" prefHeight="27.0" prefWidth="254.0" promptText="Username/E-mail" />
      <TextField fx:id="passwordField" layoutX="133.0" layoutY="228.0" prefHeight="27.0" prefWidth="186.0" promptText="Password" />
      <TextField fx:id="employeePassword" layoutX="133.0" layoutY="282.0" prefHeight="27.0" prefWidth="186.0" promptText="EmployeePassword" />
      <Button layoutX="65.0" layoutY="415.0" mnemonicParsing="false" onAction="#handleBack" prefHeight="46.0" prefWidth="98.0" text="Back">
         <font>
            <Font name="AppleMyungjo Regular" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
